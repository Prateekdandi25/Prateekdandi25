#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int a,b,r,lcm;
    cout<<"enter two numbers"<<endl;
    cin>>a>>b;
    while(true)
    {
    lcm=a;
     r=a%b;
     if(r==0)
     {
         break;
     }
     a=a*2;
        
    }
    cout<<"lcm is "<<lcm<<endl;
    return 0;
}
